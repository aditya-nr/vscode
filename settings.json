{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.fontSize": 18,
  "vim.leader": "<Space>",
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<ESC>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-h>"
      ],
      "after": [
        "<C-w>",
        "h"
      ]
    },
    {
      "before": [
        "<C-j>"
      ],
      "after": [
        "<C-w>",
        "j"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "after": [
        "<C-w>",
        "k"
      ]
    },
    {
      "before": [
        "<C-l>"
      ],
      "after": [
        "<C-w>",
        "l"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t",
        "t"
      ],
      "commands": [
        ":tabnew"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t",
        "n"
      ],
      "commands": [
        ":tabnext"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t",
        "p"
      ],
      "commands": [
        ":tabprev"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t",
        "o"
      ],
      "commands": [
        ":tabo"
      ]
    },
    {
      "before": [
        "leader",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<Leader>",
        "p"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<Leader>",
        "a"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "<Leader>",
        "p"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<Leader>",
        "r"
      ],
      "commands": [
        "code-runner.run"
      ]
    },
    {
      "before": [
        "=",
        "="
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "5",
        "j"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "5",
        "k"
      ]
    },
    {
      "before": [
        "<Leader>",
        "j"
      ],
      "after": [
        "J"
      ]
    }
  ],
  "terminal.integrated.fontFamily": "Fira Code Light , Hack Nerd Font",
  "editor.cursorBlinking": "expand",
  "editor.fontFamily": "'Fira Code Light'",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "material-icon-theme",
  "explorer.confirmDelete": false,
  "terminal.integrated.fontSize": 15,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "editor.minimap.enabled": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "terminal.integrated.defaultProfile.linux": "bash",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "vscode-edge-devtools.fallbackRevision": "@a41122be052fe9616f0def5fe6842fa942930d46",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "vim.commandLineModeKeyBindings": [],
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "vscode-edge-devtools.cssMirrorContent": true,
  "editor.wordWrap": "on",
  "window.zoomLevel": 2,
  "code-runner.respectShebang": false,
  "window.menuBarVisibility": "toggle",
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": false
}